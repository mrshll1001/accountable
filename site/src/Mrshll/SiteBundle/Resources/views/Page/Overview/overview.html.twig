{% extends 'MrshllSiteBundle:Page:base.html.twig' %}


{% block title %}Overview{% endblock title %}



{% block content %}

<div class="container">
  <h1>Overview <small>Ooooh look at the graph!</small></h1>




  <div class="alchemy" id="alchemy" style="height:300px">

  </div>

</div>

<script type="text/javascript" src="http://cdn.graphalchemist.com/alchemy.min.js"></script>
<script>
  var nodes = {{nodes.nodes|json_encode|raw}};
  var edges = {{nodes.edges|json_encode|raw}};
  var data =
      {
        "nodes": nodes,
        "edges": edges
      };

      alchemy.begin({"dataSource": data, "forceLocked": false, "nodeCaption": function(node){ return node.name}});
</script>

{% endblock content %}
